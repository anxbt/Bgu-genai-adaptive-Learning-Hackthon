# Your Gemini API keys (comma-separated for fallback rotation)
# When one key hits rate limit, the service will try the next one
GEMINI_API_KEY=your_primary_key_here
GEMINI_API_KEY_2=your_backup_key_1_here
GEMINI_API_KEY_3=your_backup_key_2_here

# Server port
PORT=3000
